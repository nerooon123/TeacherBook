<Window x:Class="TeacherBook.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeacherBook"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Image Source="/Assets/Images/wsr-300.png" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <TextBlock Text="HELLO" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="60"/>
        <Button x:Name="CanGoBack" Content="Назад" HorizontalAlignment="Right" VerticalAlignment="Center" Padding="20" Click="CanGoBack_Click"/>
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
